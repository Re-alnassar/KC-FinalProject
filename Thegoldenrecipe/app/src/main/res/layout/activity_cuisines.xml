<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fourth"
    tools:context=".Cuisines">

    <ImageButton
        android:id="@+id/arabianCuisine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="134dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="442dp"
        android:backgroundTint="#00FFFFFF"
        app:srcCompat="@drawable/pink"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/italianCuisine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="264dp"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="320dp"
        android:backgroundTint="#00FFFFFF"
        app:srcCompat="@drawable/second_cuisine"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/indianCuisine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="47dp"
        android:layout_marginTop="397dp"
        android:layout_marginBottom="186dp"
        android:backgroundTint="#00FFFFFF"
        app:srcCompat="@drawable/third_cuisine"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/seaCuisine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="530dp"
        android:layout_marginEnd="49dp"
        android:layout_marginBottom="50dp"
        android:backgroundTint="#00FFFFFF"
        app:srcCompat="@drawable/fourth_cuisine"
        tools:ignore="SpeakableTextPresentCheck" />
</RelativeLayout>